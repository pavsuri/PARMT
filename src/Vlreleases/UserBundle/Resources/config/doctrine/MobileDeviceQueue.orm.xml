<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    
    <entity table="mobiledevice_queue" name="Vlreleases\UserBundle\Entity\MobileDeviceQueue">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
        </id>
        
        <many-to-one field="mobileDevice" target-entity="Vlreleases\UserBundle\Entity\MobileDevice" inversed-by="requesters">
            <join-column name="mobiledevice_id" referenced-column-name="id" />
        </many-to-one>
        
        <many-to-one field="requester" target-entity="Vlreleases\UserBundle\Entity\User">
            <join-column name="user_id" referenced-column-name="id" />
        </many-to-one>
        
        <field name="startTime" column = "starttime" type="datetime"/>
    </entity>
</doctrine-mapping>