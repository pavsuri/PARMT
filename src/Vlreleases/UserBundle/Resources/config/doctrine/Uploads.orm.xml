<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">    
    <entity table="uploads" name="Vlreleases\UserBundle\Entity\Uploads">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
        </id>  
        <field name="userId" column = "user_id" type="integer"/>      
        <field name="fileName" column = "filename" type="string"/>
        <field name="uniqueId" column = "unique_id" type="string"/>
        <field name="uploadedDate" column = "uploadeddate" type="datetime"/>        
    </entity>
</doctrine-mapping>