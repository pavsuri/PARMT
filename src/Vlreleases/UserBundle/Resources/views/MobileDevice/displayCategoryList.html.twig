<form id='displayCategoryListform' action='{{path('vlreleases_categories_register')}}' method='POST'>
    <table class="table table-bordered table-striped">

        <thead class="table-head">
            <tr>
                <th>S.No</th>
                <th>Category Name</th>
                <th>Status</th>
                {% if(userData.rolePermission.getEdit == 'true') %}
                    <th>Edit</th>
                {% endif %}
            </tr>
        </thead>
        <tbody>
            {% set counter = 1 %}
            {% for category in categoryListData %}
                <tr>
                    <td>{{counter}}</td>
                    <td>{{category.getName}}</td>
                    <td>
                        {% if category.getActive%}
                            Active
                        {% else %}
                            Inactive
                        {%endif%}
                    </td>
                    {% if category.getAvailable%}
                        {% if(userData.rolePermission.getEdit == 'true') %}
                                <td class="dlEdit"><span class='editDevice2' id='{{category.getId}}'>Edit</span></td>
                        {% else %}
                               
                        {% endif %}
                    {% else %}    
                        <td>In use</td>
                        <td>In use</td>

                    {%endif%}
                </tr>
                {% set counter = counter + 1 %}
            {% endfor %}
            {% if counter == 1%}
                <tr>
                    <td colspan="11" align = "center">No search results found</td>
                </tr>
            {%endif%}
        </tbody>
    </table>	
    <span style='display:none;' id='submitUrl2'>{{path('vlreleases_categories_register')}}</span>
    <span style='display:none;' id='submitDeleteUrl2'>{{path('vlreleases_categories')}}</span>
    <span style='display:none;' id='listDeviceUrl2'>{{path('vlreleases_categories_listDevice')}}</span> 
</form>